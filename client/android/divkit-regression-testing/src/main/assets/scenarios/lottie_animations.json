{
  "templates": {
    "lottie_grid": {
      "type": "grid",
      "width": "div-match-parent-size",
      "height": "div-match-parent-size",
      "paddings": {
        "left": 8,
        "top": 8,
        "right": 8,
        "bottom": 8
      },
      "background": [
        {
          "type": "solid",
          "color": "#edeada"
        }
      ],
      "column_count": 2
    },
    "lottie_image": {
      "type": "image",
      "aspect": {
        "ratio": 0.8
      },
      "content_alignment_horizontal": "center",
      "content_alignment_vertical": "center"
    },
    "lottie_gif": {
      "type": "gif",
      "aspect": {
        "ratio": 0.8
      },
      "content_alignment_horizontal": "center",
      "content_alignment_vertical": "center"
    },
    "card_title": {
      "type": "text",
      "text_alignment_horizontal": "center",
      "text_alignment_vertical": "center",
      "width": "div-match-parent-size",
      "font_size": 14,
      "font_weight": "medium",
      "line_height": 18
    },
    "lottie_card": {
      "type": "container",
      "width": {
        "type": "match_parent",
        "weight": 1
      },
      "margins": {
        "left": 4,
        "right": 4,
        "bottom": 4,
        "top": 4
      },
      "paddings": {
        "top": 4
      },
      "border": {
        "corner_radius": 32,
        "unit": "dp",
        "stroke": {
          "width": 2,
          "color": "#dead00"
        }
      },
      "orientation": "vertical"
    },
    "lottie_image_card": {
      "type": "lottie_card",
      "items": [
        {
          "type": "card_title",
          "$text": "lottie_image_text"
        },
        {
          "type": "lottie_image",
          "$id": "image_id",
          "extensions": [
            {
              "id": "lottie",
              "$params": "lottie_params"
            }
          ],
          "image_url": "https://upload.wikimedia.org/wikipedia/commons/a/a1/Johnrogershousemay2020.webp"
        }
      ]
    },
    "lottie_gif_card": {
      "type": "lottie_card",
      "items": [
        {
          "type": "card_title",
          "$text": "lottie_image_text"
        },
        {
          "type": "lottie_gif",
          "$id": "image_id",
          "scale": "fit",
          "extensions": [
            {
              "id": "lottie",
              "$params": "lottie_params"
            }
          ],
          "gif_url": "https://i.gifer.com/VhdJ.gif"
        }
      ]
    }
  },

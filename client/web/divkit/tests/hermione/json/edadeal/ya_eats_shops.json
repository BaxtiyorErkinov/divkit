{
  "templates": {
    "eddl_shops_item_image": {
      "type": "image",
      "width": {
        "type": "match_parent"
      },
      "height": {
        "type": "fixed",
        "$value": "eddl_image_height"
      },
      "content_alignment_vertical": "top",
      "$image_url": "eddl_image_url"
    },
    "eddl_shops_item_title": {
      "type": "text",
      "$text_color": "eddl_title_color",
      "$font_size": "eddl_title_font_size",
      "$line_height": "eddl_title_line_height",
      "$font_weight": "eddl_title_font_weight",
      "$max_lines": "eddl_title_max_lines",
      "$text": "eddl_title"
    },
    "eddl_shops_item_delivery_info": {
      "type": "text",
      "max_lines": 1,
      "$text_color": "eddl_delivery_color",
      "alignment_horizontal": "left",
      "$font_size": "eddl_delivery_font_size",
      "$line_height": "eddl_delivery_line_height",
      "$font_weight": "eddl_delivery_font_weight",
      "$text": "eddl_delivery"
    },
    "eddl_shops_item_delivery_icon": {
      "type": "image",
      "width": {
        "type": "fixed",
        "$value": "eddl_delivery_icon_width"
      },
      "height": {
        "type": "fixed",
        "$value": "eddl_delivery_icon_height"
      },
      "alignment_horizontal": "right",
      "$image_url": "eddl_delivery_icon_url"
    },
    "eddl_shops_item_delivery": {
      "type": "container",
      "orientation": "horizontal",
      "margins": {
        "top": 2
      },
      "items": [
        {
          "type": "eddl_shops_item_delivery_info"
        },
        {
          "type": "eddl_shops_item_delivery_icon"
        }
      ]
    },
    "eddl_shops_item_info": {
      "type": "container",
      "width": {
        "type": "match_parent"
      },
      "height": {
        "type": "match_parent"
      },
      "orientation": "overlap",
      "paddings": {
        "left": 8,
        "right": 8,
        "top": 8,
        "bottom": 8
      },
      "items": [
        {
          "type": "eddl_shops_item_title",
          "alignment_vertical": "top"
        },
        {
          "type": "eddl_shops_item_delivery",
          "alignment_vertical": "bottom"
        }
      ]
    },
    "eddl_shops_item": {
      "type": "container",
      "width": {
        "type": "fixed",
        "$value": "eddl_width"
      },
      "height": {
        "type": "fixed",
        "$value": "eddl_height"
      },
      "border": {
        "$corner_radius": "eddl_corner_radius"
      },
      "background": [
        {
          "type": "solid",
          "$color": "eddl_background_color"
        }
      ],
      "items": [
        {
          "type": "eddl_shops_item_image"
        },
        {
          "type": "eddl_shops_item_info"
        }
      ],
      "$actions": "eddl_actions",
      "$visibility_actions": "eddl_visibility_actions"
    }
  },

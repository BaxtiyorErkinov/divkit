{
    "templates": {
        "bridge_card": {
            "orientation": "vertical",
            "background": [{
                "color": "#ffffff",
                "type": "solid"
            }],
            "items": [{
                "type": "title"
            }, {
                "type": "grid",
                "$items": "brige_items",
                "column_count": 9,
                "paddings": {
                    "left": 16,
                    "right": 16
                }
            }, {
                "type": "fullsize_button"
            }],
            "height": {
                "type": "wrap_content"
            },
            "type": "container"
        },
        "bridge_name": {
            "margins": {
                "top": 12
            },
            "width": {
                "type": "wrap_content"
            },
            "height": {
                "type": "fixed",
                "value": 16,
                "unit": "sp"
            },
            "$text": "name",
            "font_size": 14,
            "type": "text",
            "text_color": "#CC000000",
            "max_lines": 1
        },
        "bridge_time": {
            "margins": {
                "top": 12
            },
            "width": {
                "type": "wrap_content"
            },
            "height": {
                "type": "fixed",
                "value": 16,
                "unit": "sp"
            },
            "$text": "time",
            "font_size": 14,
            "type": "text",
            "text_color": "#CC000000",
            "max_lines": 1
        },
        "bridge_spacer": {
            "type": "separator",
            "height": {
                "type": "fixed",
                "value": 16
            },
            "width": {
                "type": "match_parent",
                "weight": 1
            },
            "delimiter_style": {
                "color": "#00000000"
            }
        },
        "bridge_spacer_big": {
            "type": "separator",
            "height": {
                "type": "fixed",
                "value": 16
            },
            "width": {
                "type": "fixed",
                "value": 24
            },
            "delimiter_style": {
                "color": "#00000000"
            }
        },
        "bridge_wave_picture_small": {
            "margins": {
                "top": 12
            },
            "width": {
                "type": "fixed",
                "value": 16
            },
            "height": {
                "type": "fixed",
                "value": 16
            },
            "scale_type": "fit",
            "placeholder_color": "#00ffffff",
            "type": "image",
            "image_url": "https://s3.mds.yandex.net/mobile-yandex-client-android/divkit/images/wave_1col.4.png"
        },
        "bridge_wave_picture_big": {
            "margins": {
                "top": 12
            },
            "width": {
                "type": "match_parent"
            },
            "height": {
                "type": "fixed",
                "value": 16
            },
            "scale_type": "fit",
            "content_alignment_vertical": "bottom",
            "placeholder_color": "#00ffffff",
            "type": "image",
            "image_url": "https://s3.mds.yandex.net/mobile-yandex-client-android/divkit/images/wave_xl.4.png"
        },
        "title": {
            "type": "container",
            "orientation": "horizontal",
            "$items": "title_items"
        },
        "title_menu": {
            "width": {
                "type": "fixed",
                "value": 44
            },
            "placeholder_color": "#00ffffff",
            "type": "image",
            "height": {
                "type": "fixed",
                "value": 44
            }
        },
        "fullsize_button": {
            "background": [{
                "type": "solid",
                "$color": "button_color"
            }],
            "border": {
                "corner_radius": 4
            },
            "$action": "button_action",
            "text_alignment_vertical": "center",
            "text_alignment_horizontal": "center",
            "paddings": {
                "bottom": 8,
                "top": 8
            },
            "width": {
                "type": "match_parent"
            },
            "margins": {
                "left": 16,
                "right": 16,
                "top": 16,
                "bottom": 16
            },
            "type": "text",
            "font_size": 13,
            "$text": "button_text",
            "line_height": 15
        },
        "title_text": {
            "paddings": {
                "top": 12,
                "left": 16,
                "right": 16
            },
            "text_color": "#cc000000",
            "type": "text",
            "font_size": 18,
            "line_height": 24,
            "font_weight": "medium"
        }
    },

{
  "templates": {
    "train_button": {
      "type": "container",
      "orientation": "horizontal",
      "background": [
        {
          "$color": "button_color",
          "type": "solid"
        }
      ],
      "border": {
        "corner_radius": 4
      },
      "font_size": 14,
      "paddings": {
        "bottom": 6,
        "top": 6,
        "left": 8,
        "right": 12
      },
      "width": {
        "type": "fixed",
        "value": 156
      },
      "content_alignment_vertical": "center",
      "$action": "button_action",
      "items": [
        {
          "type": "image",
          "width": {
            "type": "fixed",
            "value": 24
          },
          "height": {
            "type": "fixed",
            "value": 24
          },
          "$image_url": "button_image"
        },
        {
          "$text": "button_text",
          "type": "text",
          "font_size": 13,
          "line_height": 15,
          "paddings": {
            "left": 8
          }
        }
      ]
    },
    "train_card": {
      "type": "container",
      "background": [
        {
          "color": "#ffffff",
          "type": "solid"
        }
      ],
      "height": {
        "type": "wrap_content"
      },
      "items": [
        {
          "type": "title"
        },
        {
          "$items": "tab_items",
          "switch_tabs_by_content_swipe_enabled": 1,
          "title_paddings": {
            "bottom": 8,
            "left": 16,
            "right": 16
          },
          "tab_title_style": {
            "font_weight": "medium"
          },
          "type": "tabs"
        }
      ],
      "orientation": "vertical"
    },
    "train_schedule_time": {
      "type": "text",
      "$text": "time",
      "text_color": "#80000000",
      "font_size": 14,
      "width": {
        "type": "wrap_content"
      },
      "paddings": {
        "right": 21,
        "bottom": 8,
        "top": 7
      },
      "text_alignment_vertical": "center"
    },
    "train_schedule_name": {
      "type": "text",
      "$text": "name",
      "text_color": "#CC000000",
      "font_size": 16,
      "text_alignment_vertical": "center",
      "max_lines": 3,
      "width": {
        "type": "match_parent",
        "weight": 1
      },
      "paddings": {
        "top": 5,
        "bottom": 7
      }
    },
    "train_item": {
      "type": "container",
      "$action": "item_action",
      "height": {
        "type": "wrap_content"
      },
      "items": [
        {
          "type": "grid",
          "column_count": 2,
          "$items": "train_schedule_items",
          "paddings": {
            "left": 16,
            "right": 16
          }
        },
        {
          "type": "separator",
          "paddings": {
            "top": 4,
            "left": 16,
            "right": 16
          },
          "height": {
            "type": "fixed",
            "value": 6
          }
        },
        {
          "type": "gallery",
          "$items": "buttons",
          "paddings": {
            "top": 14,
            "bottom": 14,
            "left": 16,
            "right": 16
          },
          "height": {
            "type": "fixed",
            "value": 64,
            "unit": "sp"
          }
        }
      ],
      "orientation": "vertical",
      "width": {
        "type": "match_parent"
      }
    },
    "title": {
      "type": "container",
      "$items": "title_items",
      "orientation": "horizontal"
    },
    "title_menu": {
      "type": "image",
      "height": {
        "type": "fixed",
        "value": 44
      },
      "placeholder_color": "#00ffffff",
      "width": {
        "type": "fixed",
        "value": 44
      }
    },
    "title_text": {
      "type": "text",
      "font_size": 18,
      "font_weight": "medium",
      "line_height": 24,
      "paddings": {
        "left": 16,
        "right": 16,
        "top": 12
      },
      "text_color": "#cc000000"
    }
  },
